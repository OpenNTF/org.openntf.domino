	<xc:ccLayoutOneUI>
		<xp:this.facets>
			<xc:LeftView xp:key="LeftColumn"></xc:LeftView>
			<xc:mainArea xp:key="facet_1" title="View"
				javaMethodName="getAllDocumentsByKey">
				<xc:this.description>
					<![CDATA[Outputting contents from a NotesDocumentCollection.]]>
				</xc:this.description>
				<xp:this.facets>
					<xp:button value="Run SSJS Tests" id="button1"
						xp:key="SSJSButton">
						<xp:eventHandler event="onclick" submit="true"
							refreshMode="partial" refreshId="SSJSDiv">
							<xp:this.action><![CDATA[#{javascript:try {
	var statesView:org.openntf.domino.View = database.getView("allContactsByState");
	var sb = new java.lang.StringBuilder();
	var key:java.util.ArrayList = new java.util.ArrayList();
	key.add("CA");
	var dc = statesView.getAllDocumentsByKey(key,true);
	for (doc in dc) {
		sb.append(doc.get("FirstName") + " " + doc.get("LastName") + "<br/>"); // Do whatever it is you actually want to get done
	}
	viewScope.put("SSJSTest",sb.toString());
} catch(e) {
	dump(e);
}}]]></xp:this.action>
						</xp:eventHandler>
					</xp:button>
					<xp:button value="Run Java Tests" id="button3"
						xp:key="javaButton">
						<xp:eventHandler event="onclick" submit="true"
							refreshMode="partial" refreshId="JavaDiv"
							action="#{javascript:openNTFViewBean.getAllDocumentsByKey()}">
						</xp:eventHandler>
					</xp:button>
					<xp:button value="Run Old Java Tests" id="button2"
						xp:key="oldJavaButton">
						<xp:eventHandler event="onclick" submit="true"
							refreshMode="partial" refreshId="oldJavaDiv"
							action="#{javascript:oldViewBean.getAllDocumentsByKey()}">
						</xp:eventHandler>
					</xp:button>
				</xp:this.facets>
			</xc:mainArea>
		</xp:this.facets>
	</xc:ccLayoutOneUI>
</xp:view>
